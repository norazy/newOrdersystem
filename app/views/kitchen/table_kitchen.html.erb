<div class="kitchen_background-color">
    <div class="kitchen_background">
        <header class="kitchen_header clearfix">
            <div class="kitchen_header_left">
                <div class="kitchen_button">
                    <%= link_to "ログアウト", destroy_user_session_path, method: :delete %>
                </div>
                <div class="kitchen_nav">
                    <button class="kitchen_nav_button" id="kitchen_nav_button">
                        <i class="fas fa-bars"><span></span></i>
                    </button>
                    <div class="kitchen_nav_div" id="kitchen_nav_button2">
                        <ul class="kitchen_nav_ul">
                            <li class="kitchen_nav_li"><%= link_to "メニューTOP", firstpage_path, method: :get %></li>
                            <li class="kitchen_nav_li"><a href="/kitchen/served">注文一覧</a></li>
                            <li class="kitchen_nav_li">
                                <%= link_to('中国語', :locale => 'zh') %>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="kitchen_button_right">
                <div class="kitchen_button blue">
                    <a href="/kitchen/table">
                        <p>テーブル</p>
                    </a>
                </div>
                <div class="kitchen_button">
                    <a href="/kitchen/ordered">
                        <p>調理待</p>
                    </a>
                </div>
                <div class="kitchen_button">
                    <a href="/kitchen/option_change">
                        <p>注文変更</p>
                    </a>
                </div>
                <div class="kitchen_button">
                    <a href="/kitchen/notification">
                        <p>通知一覧</p>
                    </a>
                </div>
            </div>
        </header>
        <div class="table_display">
            <div class="front_table">
                <div class="front_table_1_4">
                    <div class="table_indiv table1_4">
                        <p>4</p>
                        <span><%= @color[3] %></span>
                    </div>
                    <div class="table_indiv table1_4">
                        <p>3</p>
                        <span><%= @color[2] %></span>
                    </div>
                    <div class="table_indiv table1_4">
                        <p>2</p>
                        <span><%= @color[1] %></span>
                    </div>
                    <div class="table_indiv table1_4">
                        <p>1</p>
                        <span><%= @color[0] %></span>
                    </div>
                </div>
                <div class="front_table_5_7_n_guide">
                    <div class="front_table_5_6">
                        <div class="table_indiv table5_6">
                            <p>5</p>                            
                            <span><%= @color[4] %></span>
                        </div>
                        <div class="table_indiv table5_6">
                            <p>6</p>
                            <span><%= @color[5] %></span>
                        </div>
                    </div>
                    <div class="front_table_7_n_guide">
                        <div class="front_table_7">
                        <div class="table_indiv table7">
                                <p>7</p>
                                <span><%= @color[6] %></span>
                            </div>
                        </div>
                        <div class="front_table_n"></div>
                        <div class="front_table_guide clearfix">
                            <div class="guide">
                                <div class="guide_indiv gray">
                                    <p>無人</p>
                                </div>                                
                                <div class="guide_indiv green">
                                    <p>注文待</p>
                                </div>                                
                                <div class="guide_indiv blue2">
                                    <p>調理待</p>
                                </div>                                
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="back_table">
                <div class="back_table_8_10">
                    <div class="table_indiv table8_10">
                        <p>8</p>
                        <span><%= @color[7] %></span>
                    </div>
                    <div class="table_indiv table8_10">
                        <p>9</p>
                        <span><%= @color[8] %></span>
                    </div>
                    <div class="table_indiv table8_10">
                        <p>10</p>
                        <span><%= @color[9] %></span>
                    </div>
                </div>
                <div class="back_table_else clearfix">
                    <div class="back_table_11_16">
                        <div class="back_table_11_13">
                            <div class="table_indiv table11_13">
                                <p>11</p>
                                <span><%= @color[10] %></span>
                            </div>
                            <div class="invisible_table11_13">
                                <!--<p></p>-->
                            </div>
                            <div class="table_indiv table11_13">
                                <p>12</p>
                                <span><%= @color[11] %></span>
                            </div>
                            <div class="table_indiv table11_13">
                                <p>13</p>
                                <span><%= @color[12] %></span>
                            </div>
                        </div>
                        <div class="back_table_14_16">
                            <div class="table_indiv table14_16">
                                <p>14</p>
                                <span><%= @color[13] %></span>
                            </div>
                            <div class="table_indiv table14_16">
                                <p>15</p>
                                <span><%= @color[14] %></span>
                            </div>
                            <div class="table_indiv table14_16">
                                <p>16</p>
                                <span><%= @color[15] %></span>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

